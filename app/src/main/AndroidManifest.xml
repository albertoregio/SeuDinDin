<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.regio.seudindin">

    <application
        android:name="br.com.arsolutions.seudindin.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="br.com.arsolutions.seudindin.mainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.arsolutions.seudindin.ui.categories.CategoryListActivity"
            android:label="@string/category_list_title_edit" />
        <activity android:name="br.com.arsolutions.seudindin.ui.accounts.AccountListActivity" />
        <activity
            android:name="br.com.arsolutions.seudindin.ui.accounts.AccountDetailActivity"
            android:label="@string/title_activity_account_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="br.com.arsolutions.seudindin.ui.categories.CategoryDetailActivity"></activity>
    </application>

</manifest>